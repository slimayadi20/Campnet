                    <form id="contact-us" method="post" action="contact.php">
                        <!-- Left Inputs -->
                        <div class="col-xs-6 ">
                            <!-- Name -->
                            <br>
                            <input type="text" name="name" id="name" required="required" class="form" placeholder="Nom" />
                            <!-- Email -->
